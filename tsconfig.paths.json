{
  "compilerOptions": {
    "baseUrl": "./src",
    "paths": {
      "@Assets": ["assets"],
      "@Assets/*": ["assets/*"],

      "@Components": ["components"],
      "@Components/*": ["components/*"],

      "@Hooks": ["hooks"],
      "@Hooks/*": ["hooks/*"],

      "@Pages": ["pages"],
      "@Pages/*": ["pages/*"],

      "@Services": ["services"],
      "@Services/*": ["services/*"],

      "@Styles": ["styles"],
      "@Styles/*": ["styles/*"],

      "@Utils": ["utils"],
      "@Utils/*": ["utils/*"]
    }
  }
}
